import pandas as pd 
# please note old is the data from the last code run, new is data from this code run
old = pd.read_csv("/home/ads/generic-real-estate-consulting-project-group-55/rent_data/renew(06)")
new = pd.read_csv("/home/ads/generic-real-estate-consulting-project-group-55/rent_data/rent_time2022-09-07")
def renew_data(old, new):
    renew = pd.concat([old, new])
    renew = renew.reset_index(drop = True).drop_duplicates(subset = ['dateListed'])
    return renew
renew_data(old, new).to_csv("/home/ads/generic-real-estate-consulting-project-group-55/rent_data/renew(07)")